{
  "name": "nextjs_template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint --apply ./src",
    "fmt": "biome format --write ./src",
    "check": "biome check --apply ./src",
    "check:precommit": "biome check ./src",
    "check:unsafe": "biome check --apply-unsafe ./src"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "18.17.5",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "lefthook": "1.6.10",
    "typescript": "5.4.5"
  }
}
